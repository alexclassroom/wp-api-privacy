{
    "name": "wp-privacy/wp-api-privacy",
    "type": "wordpress-plugin",
    "description": "Strips potentially identifying information from outbound requests to the WordPress.org API",
    "license": "GPL-3.0-or-later",
    "keywords": [ "wordpress", "plugin", "privacy" ],
    "authors": [
        { "name": "Duane Storey", "email": "duanestorey@gmail.com" }
    ],
    "require": {
        "composer/installers": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }   
    }
}
